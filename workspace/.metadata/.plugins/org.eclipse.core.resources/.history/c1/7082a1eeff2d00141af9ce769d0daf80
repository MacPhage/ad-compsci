import javax.swing.JOptionPane;



public class DayOne
{
	public static String output  = "";

	public static byte b = 127;
	public static short s = 24;
	public static int i = 892;
	public static long l = 1283921983;
	public static float f = 124.2f;
	public static double d = 12321.21321;
	public static char c = 'A';
	public static boolean bool = false;
	public static String str = "Words!";
	
	public static void main ( String[] args )
	{

		output = "/////////////////////////////////"+
		"*Some Person            07/18/02*"+
		"*                               *"+
		"*        integer types          *"+
		"*                               *"+
		"*8 bit - byteOne = "+b+"          *"+
		"*16 bit - shortOne = "+s+"     *"+
		"*32 bit - intOne = "+i+"        *"+
		"*64 bit - longOne = "+l+"   *"+
		"*                               *"+
		"*         real types            *"+
		"*                               *"+
		"*32 bit - floatOne = "+f+"    *"+
		"*64 bit - doubleOne = "+d+"   *"+
		"*                               *"+
		"*16 bit - charOne = "+c+"           *"+
		"*                               *"+
		"*         other types           *"+
		"*                               *"+
		"*booleanOne = "+bool+"              *"+
		"*stringOne = "+str+"        *"+
		"/////////////////////////////////";
		
		System.out.println(output);
		JOptionPane.showMessageDialog(null, output);
		
	}
	
}
